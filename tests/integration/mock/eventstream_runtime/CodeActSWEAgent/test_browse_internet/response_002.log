import re
from bs4 import BeautifulSoup

html_content = """
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>The Ultimate Answer</title>
    <style>
        /* ... (style content omitted for brevity) ... */
    </style>
</head>
<body>
    <div class="container">
        <h1>The Ultimate Answer</h1>
        <p>Click the button to reveal the answer to life, the universe, and everything.</p>
        <button id="showButton">Click me</button>
        <div id="result"></div>
    </div>
    <script>
        document.getElementById('showButton').addEventListener('click', function() {
            document.getElementById('result').innerText = 'The answer is OpenHands is all you need!';
        });
    </script>
</body>
</html>
"""

soup = BeautifulSoup(html_content, 'html.parser')
script_content = soup.find('script').string
match = re.search(r"document\.getElementById\('result'\)\.innerText = '(.+)';", script_content)
if match:
    ultimate_answer = match.group(1)
    print(f"The ultimate answer to life is: {ultimate_answer}")
else:
    print("Could not find the ultimate answer in the script.")
